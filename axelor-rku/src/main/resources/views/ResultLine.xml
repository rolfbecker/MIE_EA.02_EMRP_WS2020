<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<grid name="result-line-grid" title="ResultLine" model="com.axelor.apps.rku.db.ResultLine" editable="true">
		<hilite if="point  &lt; 50" color="danger"/>
		 <field name="student" readonly="true"/>
		 <field name="point" />
		 <field name="exam" readonly="true"/>
		 <field name="subject" readonly="true"/>
		 <field name="semesterConfig" readonly="true"/>
		 <field name="branchConfig" readonly="true"/>
	</grid>
	
	<form name="result-line-form" title="ResultLine" model="com.axelor.apps.rku.db.ResultLine" editable="true">
		
		<panel name="mainResultLinePanal">
			 <field name="student" readonly="true" canView="false" />
			 <field name="point" readonly="true"/>
			 <field name="exam" readonly="true"/>
			 <field name="subject" readonly="true" canView="false"/>
			 <field name="semesterConfig" readonly="true" canView="false"/>
			 <field name="branchConfig" readonly="true" canView="false" />
		</panel>

	</form>
	
</object-views>
