<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<grid name="semester-config-grid" title="Semester config" model="com.axelor.apps.rku.db.SemesterConfig">
		 <field name="name"/>
		 <field name="code" />
	</grid>

	<form name="semester-config-form" title="Semester config" model="com.axelor.apps.rku.db.SemesterConfig" >
		
		<panel name="mainSemConfigPanal">
			<field name="name" required="true" />
			<field name="code"/>
			<field name="sem"/>
		</panel>
		
	</form>
</object-views>
