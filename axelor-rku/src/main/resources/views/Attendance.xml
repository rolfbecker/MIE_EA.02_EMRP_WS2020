<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	<grid name="attendance-grid" title="Attendance" model="com.axelor.apps.rku.db.Attendance">
		 <field name="subject" />
		 <field name="attendanceDate"/>
		 <field name="semesterConfig"/>
		 <field name="branchConfig"/>
		 <field name="userFaculty"/>
	</grid>

	<form name="attendance-form" title="Attendance" model="com.axelor.apps.rku.db.Attendance" width="large">
		
		<panel name="mainAttendancePanal">
			 <field name="userFaculty"/>
			 <field name="subject" />
			 <field name="attendanceDate"/>
			 <field name="semesterConfig"/>
			 <field name="branchConfig"/>
		</panel>
		
		<panel-related field="attendanceLine" canEdit="true"/>
		
	</form>
</object-views>
