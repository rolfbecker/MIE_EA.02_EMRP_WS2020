<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">
	
	
	<dashboard title="Calls Dashboard" name="rku.calls.dashboard"  >
		<dashlet action="dashlet.chart.dashboard.student.class.attendance.bar" height="350"  colSpan="12" />
		<dashlet action="dashlet.chart.dashboard.student.subject.attendance.bar" height="350"  colSpan="12" />
	</dashboard>
	
	<action-view name="dashlet.chart.dashboard.student.class.attendance.bar" title="Class Attendance">
		<view type="chart" name="chart.dashboard.student.class.attendance.bar"/>
	</action-view>
	
	<action-view name="dashlet.chart.dashboard.student.subject.attendance.bar" title="Class Attendance">
		<view type="chart" name="chart.dashboard.student.subject.attendance.bar"/>
	</action-view>



	<dashboard title="Account Dashboard" name="rku.account.dashboard" >
		<dashlet action="dashlet.chart.dashboard.account.tution.fee.per.course.bar" height="350"  colSpan="12" />
		<dashlet action="dashlet.chart.dashboard.account.exam.fee.per.course.bar" height="350"  colSpan="12" />
	</dashboard>
	
	<action-view name="dashlet.chart.dashboard.account.tution.fee.per.course.bar" title="Course per Fees">
		<view type="chart" name="chart.dashboard.account.amount.per.course.tution.fee.bar"/>
	</action-view>
	
	<action-view name="dashlet.chart.dashboard.account.exam.fee.per.course.bar" title="Course per Fees">
		<view type="chart" name="chart.dashboard.account.amount.per.course.exam.fee.bar"/>
	</action-view>

</object-views>
