<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="rku" package="com.axelor.apps.rku.db"/>

  <entity name="Subject" cachable="true">
  
  	<string name="name" namecolumn="true" title="Subject Name" required="true" />
  	<string name="code" title="Code" required="true" unique="true" />
  	<integer name="subjectCredit" title="Credit" required="true"/>
    <boolean name="active" title="Active" />
    <many-to-one name="branchConfig" ref="com.axelor.apps.rku.db.BranchConfig" />
    <many-to-one name="semesterConfig" ref="com.axelor.apps.rku.db.SemesterConfig" />
  </entity>
  
</domain-models>





