<?xml version="1.0" ?>
<domain-models xmlns="http://axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.0.xsd">

  <module name="rku" package="com.axelor.apps.rku.db"/>

  <entity name="Course" cachable="true">
      
      <string name="name"  title="CourseName" namecolumn="true"/>
      <string name="code" title="Code" />
      <integer name="totalSem" title="Total Semester"/>
      
      <string name="year" title="Year"/>
      <string name="semester" selection="rku.semester.odd.even.select" title="Semester type"/>
      <string name="currentYear" title="Current year" />
      
      <boolean name="setCurrentYear" />
    
  </entity>
  
</domain-models>
