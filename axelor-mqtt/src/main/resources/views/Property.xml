<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid name="property-grid" title="Property" model="com.axelor.apps.mqtt.db.Property">
		<field name="name" />
		<field name="unit"></field>
		<field name="discription"/>
		<field name="device"></field>
	</grid>

	<form name="property-form" title="Property"
		model="com.axelor.apps.mqtt.db.Property">

		<panel>
			<field name="id" />
			<field name="name" />
			<field name="discription" />
			<field name="device"></field>
		</panel>
		
		<panel-related field="data" title="Data"/>

	</form>
</object-views>
